{
  "name": "andrewginns.github.io",
  "type": "module",
  "version": "1.0.0",
  "description": "Andrew Ginns - Machine Learning Engineer Personal Website",
  "scripts": {
    "dev": "astro dev",
    "start": "astro dev",
    "build": "astro check && npm run typecheck && astro build",
    "build:watch": "astro check --watch",
    "preview": "astro preview",
    "astro": "astro",
    "prepare": "husky",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "typecheck": "tsc --noEmit",
    "typecheck:watch": "tsc --noEmit --watch",
    "validate": "npm run typecheck && npm run lint && npm run format:check"
  },
  "keywords": [
    "machine-learning",
    "portfolio",
    "blog"
  ],
  "author": "Andrew Ginns",
  "license": "MIT",
  "dependencies": {
    "@astrojs/check": "^0.9.4",
    "@astrojs/sitemap": "^3.4.1",
    "astro": "^5.9.4",
    "astro-robots-txt": "^1.0.0",
    "astro-seo": "^0.8.4",
    "astro-seo-schema": "^5.0.0",
    "fast-xml-parser": "^5.2.5",
    "typescript": "^5.8.3"
  },
  "devDependencies": {
    "@playwright/test": "^1.53.1",
    "@types/node": "^24.0.3",
    "@typescript-eslint/eslint-plugin": "^8.34.1",
    "@typescript-eslint/parser": "^8.34.1",
    "eslint": "^9.29.0",
    "eslint-config-prettier": "^10.1.5",
    "eslint-plugin-astro": "^1.3.1",
    "husky": "^9.1.7",
    "jiti": "^2.4.2",
    "lint-staged": "^16.1.2",
    "prettier": "^3.5.3",
    "prettier-plugin-astro": "^0.14.1",
    "typescript-eslint": "^8.34.1"
  }
}
